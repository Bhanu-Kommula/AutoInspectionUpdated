# Production Environment Variables for AutoInspect Frontend
# Copy this to .env.production and set the values in Render

# API Endpoints (set these in Render environment variables)
REACT_APP_API_GATEWAY_URL=https://api-gateway.onrender.com
REACT_APP_CHAT_SERVICE_URL=https://chat-service.onrender.com
REACT_APP_WEBSOCKET_URL=wss://chat-service.onrender.com

# Feature Flags
REACT_APP_ENABLE_WEBSOCKET=true
REACT_APP_ENABLE_CHAT=true
REACT_APP_ENABLE_CALLS=true

# Analytics (optional)
REACT_APP_ENABLE_ANALYTICS=false
